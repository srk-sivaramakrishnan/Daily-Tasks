{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/users(.*)",   
      "dest": "/server.js"   
    },
    {
      "src": "/pages(.*)",    
      "dest": "/server.js"   
    },
    {
      "src": "/(.*)",         
      "dest": "/server.js"    
    }
  ]
}
